<t tx="zoomq.20160415140158.33">def scan_for_configs (self,p,d):
    
    '''Scan the body text of p, finding all calls to getX.'''
    kinds = ('getBool','getColor','getInt','getFloat',
        # '@font', # special case.
        'getRatio','getPath','getString',
    )
    i,s = 0,p.b
    while i &lt; len(s):
        progress = i
        ch = s[i]
        if (
            ch == '@' and
            (g.match(s,i,'@ ') or g.match(s,i,'@\n')) and 
            (i == 0 or s[i-1] == '\n')
        ):
            i = s.find('\n@c',i)
            if i == -1: break
        elif ch == '#':
            i = g.skip_to_end_of_line(s,i)
        elif ch in ('"',"'"):
            i = g.skip_string(s,i,verbose=False)
        elif ch == '_' or ch.isalpha():
            j = g.skip_id(s,i)
            kind = s[i:j]
            if kind in kinds:
                i = g.skip_ws(s,j)
                if g.match(s,i,'('):
                    i = g.skip_ws(s,i+1)
                    if g.match(s,i,'"') or g.match(s,i,"'"):
                        j = g.skip_string(s,i)
                        name = s[i+1:j-1]
                        aList = d.get(kind,[])
                        if name not in aList:
                            aList.append(name)
                        d [kind] = aList
            i = j
        else:
            i += 1
        assert progress &lt; i
    return d</t>
